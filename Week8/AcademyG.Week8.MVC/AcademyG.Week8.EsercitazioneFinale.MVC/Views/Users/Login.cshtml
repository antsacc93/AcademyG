@model UserViewModel
<div class="container-fluid">

    <div class="row mb-2 mb-lg-0">

        <div class="col-4"></div>
        <form method="post">
            <partial name="_FormUser" model="Model" />

            <input type="submit" value="Login" class="btn btn-primary" />
            Or
            <a role="button" class="btn btn-success" asp-controller="Users" asp-action="Register">Register</a>

        </form>
    </div>
</div>

